msgstr "algoritmo"

msgid "iterations"
msgstr "iterazioni"

msgid "salt"
msgstr "salt"

msgid "hash"
msgstr "hash"

msgid "work factor"
msgstr "work factor"

msgid "checksum"
msgstr "checksum"

msgid "name"
msgstr "nome"

msgid "codename"
msgstr "nome in codice"

msgid "permission"
msgstr "permesso"

msgid "permissions"
msgstr "permessi"

msgid "group"
msgstr "gruppo"

msgid "groups"
msgstr "gruppi"

